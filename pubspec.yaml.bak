name: rail_bottom_navigation_flutter
description: rail_bottom_navigation_flutter
publish_to: 'none' # Remove this line if you wish to publish to pub.dev
version: 1.0.0+1

environment:
  sdk: '>=2.19.0 <3.0.0'

dependencies:
  flutter:
    sdk: flutter

  cupertino_icons: ^1.0.5
  flutter_launcher_icons: ^0.12.0
  package_info_plus: ^3.0.3
  connectivity_plus: ^3.0.3
  webview_flutter: ^4.0.6
  path_provider: ^2.0.13
  side_navigation: ^1.0.4
  easy_localization: ^3.0.1
#  shared_preferences: ^2.0.18
#  intl: any

dev_dependencies:
  flutter_test:
    sdk: flutter
  flutter_lints: ^2.0.1

flutter:
  uses-material-design: true
  assets:
  - assets/
  - assets/720x1520-background.jpg
  - assets/l10n/hu.png
  - assets/l10n/en.png
  - assets/l10n/de.png
  - assets/hagyomanyok_haza_demo.png

flutter_icons:
  android: "launcher_icon"
  ios: true
  image_path: "assets/app_icon.jpg"
  min_sdk_android: 21 # android min sdk min:16, default 21